{
  "name": "reactsetup",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "author": "brightcr",
  "license": "MIT",
  "description": "Simple demo setup of ReactJS with typescipt and webpack",
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.2",
    "enzyme": "^2.7.1",
    "jest": "*",
    "react-addons-test-utils": "*",
    "source-map-loader": "^0.1.6",
    "typescript": "^2.1.5",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@types/react": "^15.0.6",
    "@types/react-dom": "^0.14.22",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/Tests/*.(ts|tsx|js)"
    ]
  }
}
